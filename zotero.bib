@article{millet_modelling_2002,
	title = {Modelling risk and uncertainty with the analytic hierarchy process},
	volume = {11},
	issn = {1099-1360},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/mcda.319},
	doi = {https://doi.org/10.1002/mcda.319},
	abstract = {This paper proposes methods for modelling risk and uncertainty with the analytic hierarchy process ({AHP}). We start by showing why benefit/risk ratios, as described in previous literature, might be an improper modelling approach. We then introduce prototypical case studies where risk plays a role in multicriteria decision making. These cases demonstrate how the {AHP} can be used to derive relative probabilities, multiple criteria outcome measures, risk criteria, and risk adjustment factors. Copyright © 2003 John Wiley \& Sons, Ltd.},
	pages = {97--107},
	number = {2},
	journaltitle = {Journal of Multi-Criteria Decision Analysis},
	author = {Millet, Ido and Wedley, William C.},
