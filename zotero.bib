@article{wendt_reaction_1986,
	title = {Reaction kinetics and reaction techniques for mediated oxidation of methylarenes to aromatic ketones},
	volume = {16},
	issn = {1572-8838},
	url = {https://doi.org/10.1007/BF01015994},
	doi = {10.1007/BF01015994},
	abstract = {The Mn3+-mediated anodic oxidation of toluene and some substituted toluenes (p-chloro-,p-xylene-,p-nitro-) in 60 wt \% H2SO4 has been investigated with respect to reaction kinetics and process techniques. The mediators Co3+ in 3.5 N {HNO}3 and Ce4+ in 60 wt \% H2SO4 are compared with Mn3+ in 60 wt \% H2SO4 for unsubstituted toluene. The mediated oxidation of the toluenes proceeds in three well-distinguished steps: benzyl alcohol, benzaldehyde, benzoic acid. The last step, which destroys the desired product, the aldehyde, is crucial. Therefore, for continuous resaturation of the electrolyte with toluene and for extraction of benzaldehyde, the benzaldehyde content in the emulgated organic phase has to be kept below a critical value. For toluene,p-chlorotoluene and xylene Mn3+-mediated oxidation only becomes attractive with process temperatures above 60°C. The conversion ofp-nitrotoluene top-nitrobenzaldehyde can only be carried out by Co3+. The reactivities of the oxidants declines in the order: Co3+{\textgreater}Mn3+{\textgreater}Ce4+. Current efficiencies for the recuperation of all oxidants approaches 100\%.},
	pages = {134--146},
	number = {1},
	journaltitle = {Journal of Applied Electrochemistry},
	shortjournal = {J. Appl. Electrochem.},
	author = {Wendt, H. and Schneider, H.},
	urldate = {2021-02-06},
	date = {1986-01-01},
	langid = {english},
}

